package jp.cellfusion.ui.video{	import flash.display.Sprite;	import flash.events.IEventDispatcher;	import flash.media.SoundTransform;	import flash.net.URLRequest;	/**	 * @author Mk-10:cellfusion	 * 	 */	public interface IVideoPlayer extends IEventDispatcher	{		/**		 * Video を再生する		 */		function play(request:URLRequest = null):void;		/**		 * Video を停止する		 */		function pause():void;		function stop():void;		function resume():void;		function togglePause():void;		function load(request:URLRequest):void;		/**		 * Video を読み込む		 */		//		function load(url:String):void;		function seek(offset:Number):void;		function close():void;		function get time():Number;		function get duration():Number;		function get bytesLoaded():Number;		function get bytesTotal():Number;		function get bufferLength():Number;		function get bufferTime():Number;		function get soundTransform():SoundTransform;		function get metadata():Object;		function get volume():Number;		function set volume(offset:Number):void;		function get autoRewind():Boolean;		function set autoRewind(value:Boolean):void;		/**		 * ユーザーが SeekBar でスクラブ中は true になり、それ以外の場合は false になります。 		 * スクラブとは、シークバーのハンドルをつかみ、FLV ファイル内の特定のシーンを見つけるためにいずれかの方向にドラッグする操作のことです。		 */		function get scrubbing():Boolean;		function get preferredHeight():Number;		function get preferredWidth():Number;		/**		 * ui をまとめた DisplayObject を指定する		 */		function set ui(value:Sprite):void;		/**		 * playButton(再生ボタン)を指定する		 */		function set playButton(value:Sprite):void;		/**		 * pauseButton(停止ボタン)を指定する		 */		function set pauseButton(value:Sprite):void;		/**		 * 再生ボタンと停止ボタンがセットになっているボタンを指定する		 * どちらか片一方は visible = false; になる		 */		function set playPauseButton(value:Sprite):void;		function set stopButton(value:Sprite):void;		function set seekBar(value:Sprite):void;		function set volumeBar(value:Sprite):void;		function set muteButton(value:Sprite):void;		function set backButton(value:Sprite):void;		function set forwordButton(value:Sprite):void;		function get smoothing():Boolean;		function set smoothing(value:Boolean):void;	}}